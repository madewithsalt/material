{
  "name": "material",
  "version": "1.0.7",
  "description": "A Material Design inspired library of SASS styles and React Components working in harmony.",
  "main": "lib/all.js",
  "scripts": {
    "watch": "npm-watch",
    "test": "jest",
    "start": "brunch watch --server",
    "build": "brunch build --production && mkdir -p lib && npm run build:components",
    "deploy": "git push origin :gh-pages && git subtree push --prefix build origin gh-pages",
    "build:components": "babel app/js/all.js > lib/all.js && babel app/js/components -d lib/components"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tnbKristi/material.git"
  },
  "author": "Kristi Centinaro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tnbKristi/material/issues"
  },
  "jest": {
    "automock": false
  },
  "watch": {
    "build": {
      "patterns": [
        "app/js/**/*.js"
      ],
      "extensions": "js,sass,html",
      "ignore": "app/vendor/**/*"
    }
  },
  "homepage": "https://github.com/tnbKristi/material#readme",
  "devDependencies": {
    "auto-reload-brunch": "^2",
    "autoprefixer": "^7.1.1",
    "babel-brunch": "^6.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "brunch": "^2",
    "clean-css-brunch": "^2.10.0",
    "enzyme": "^2.9.1",
    "faker": "^4.1.0",
    "isdev": "^1.0.1",
    "javascript-brunch": "^2.10.0",
    "jest": "^20.0.4",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "npm-watch": "^0.2.0",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^15.6.1",
    "sass-brunch": "^2.10.4",
    "sinon": "^2.3.6",
    "uglify-js-brunch": "~2.0.1"
  },
  "dependencies": {
    "datatables.net": "^1.10.15",
    "jquery": "^3.2.1",
    "material-design-icons": "^3.0.1",
    "prismjs": "^1.6.0",
    "whatwg-fetch": "^2.0.3"
  }
}
